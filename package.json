{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Zane Adix <zaneadix@gmail.com>",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "clean": "gatsby clean",
    "develop": "yarn clean && nodemon --exec \"gatsby develop --host=0.0.0.0\"",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "fileSystemPath": "",
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "dependencies": {
    "gatsby": "^2.4.3",
    "gatsby-background-image": "^0.10.0",
    "gatsby-image": "^2.0.41",
    "gatsby-plugin-emotion": "4.0.7",
    "gatsby-plugin-google-analytics": "2.1.1",
    "gatsby-plugin-layout": "1.0.15",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify-cms": "4.0.1",
    "gatsby-plugin-netlify-cms-paths": "1.2.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-plugin-svg-sprite": "^2.0.1",
    "gatsby-plugin-typography": "2.2.13",
    "gatsby-source-filesystem": "2.0.33",
    "gatsby-transformer-remark": "2.3.12",
    "gatsby-transformer-sharp": "^2.1.19",
    "netlify": "^3.0.0",
    "netlify-cli": "^2.32.0",
    "netlify-cms-app": "2.9.1",
    "netlify-cms-backend-fs": "^0.4.4",
    "nodemon": "^2.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-emotion": "10.0.0",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.3.1",
    "react-typography": "0.16.19",
    "react-youtube": "^7.9.0",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "prettier": "^1.17.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
